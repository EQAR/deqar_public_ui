 <footer class="footer">
    <div class="footer__container u-container">
        <div class="footer__column footer__column--large">
            {% if footer.title %}
                <h3 class="footer__title footer__title--trademark">{{ footer.title }}</h3>
            {% endif %}

            {% if footer.text %}
                <div class="footer__text">{{ footer.text }}</div>
            {% endif %}

            <!-- Matomo -->
            <noscript>
                <img src="//analytics.eqar.eu/piwik.php?idsite=1&rec=1" style="border:0;" alt="" />
            </noscript>
            <!-- End Matomo Code -->
        </div>

        <div class="footer__column">
            {% if footer.navigation_title %}
                <h3 class="footer__title">{{ footer.navigation_title }}</h3>
            {% endif %}

            <ul class="footer__links">
                {% if function('is_user_logged_in') %}
                    <li class="menu-item menu-item--login">
                        <a href="{{ fn('wp_logout_url', site.url) }}" class="menu-link">Logout</a>
                    </li>
                {% else %}
                    {% set login = fn('get_the_permalink', login.internal_area) %}

                    <li class="menu-item menu-item--login">
                        <a href="{{ fn('wp_login_url', login) }}" class="menu-link">Committee login</a>
                    </li>
                {% endif %}

                {% for item in footer_menu.get_items %}
                    <li class="{{ item.classes | join(' ') }}">
                        <a href="{{ item.get_link }}" class="menu-link">{{ item.title }}</a>
                    </li>
                {% endfor %}
            </ul>
        </div>
    </div>
</footer>

<footer class="subfooter">
    <div class="subfooter__container u-container">
        <ul class="contact-details">
            <li class="contact-details__item">
                {{ bloginfo('name') }}    
            </li>
            {% if contact.telephone %}
                <li class="contact-details__item">
                    Tel. <a href="tel:{{ contact.telephone }}">{{ contact.telephone }}</a>    
                </li>
            {% endif %}
            {% if contact.email %}
                <li class="contact-details__item">
                    <a href="mailto:{{ contact.email }}">{{ contact.email }}</a>    
                </li>
            {% endif %}
        </ul>

        <ul class="social">
            {% if social.facebook %}
                <li class="social__item">
                    <a href="{{ social.facebook }}" target="_blank" class="social__link social__link--facebook">
                        <svg xmlns="http://www.w3.org/2000/svg" width="9" viewBox="0 0 13 25" class="social__icon">
                            <path d="M8.44,25V13.6h3.83l.57-4.45H8.44V6.31c0-1.29.36-2.16,2.21-2.16H13v-4A32,32,0,0,0,9.57,0C6.17,0,3.84,2.07,3.84,5.87V9.15H0V13.6H3.84V25Z"/>
                        </svg>
                        Facebook
                    </a>
                </li>
            {% endif %}
            {% if social.twitter %}
                <li class="social__item">
                    <a href="{{ social.twitter }}" target="_blank" class="social__link">
                        <svg xmlns="http://www.w3.org/2000/svg" width="15" viewBox="0 0 22 18" class="social__icon">
                            <path d="M19.41,2.85a4.55,4.55,0,0,0,2-2.51,9,9,0,0,1-2.87,1.1,4.51,4.51,0,0,0-7.81,3.11,4.59,4.59,0,0,0,.12,1A12.79,12.79,0,0,1,1.53.83,4.57,4.57,0,0,0,2.93,6.9a4.47,4.47,0,0,1-2-.57v.06A4.54,4.54,0,0,0,4.5,10.84a4.49,4.49,0,0,1-2,.08,4.52,4.52,0,0,0,4.22,3.16A9,9,0,0,1,1.08,16,9.12,9.12,0,0,1,0,16a12.71,12.71,0,0,0,6.92,2A12.8,12.8,0,0,0,19.76,5.07q0-.3,0-.59A9.21,9.21,0,0,0,22,2.13,9,9,0,0,1,19.41,2.85Z"/>
                        </svg>
                        Twitter
